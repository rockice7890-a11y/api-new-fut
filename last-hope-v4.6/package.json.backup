{
  "name": "hotel-management-system",
  "version": "1.0.0",
  "description": "Comprehensive hotel booking and management API system",
  "private": true,
  "scripts": {
    "build": "pnpm dlx prisma generate && next build",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "db:push": "pnpm dlx prisma db push",
    "db:migrate": "pnpm dlx prisma migrate dev",
    "db:migrate:prod": "pnpm dlx prisma migrate deploy",
    "db:generate": "pnpm dlx prisma generate",
    "db:studio": "pnpm dlx prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "pnpm dlx prisma migrate reset",
    "test": "node tests/comprehensive-api-tests.js",
    "test:functional": "node tests/comprehensive-api-tests.js functional",
    "test:performance": "node tests/comprehensive-api-tests.js performance",
    "test:security": "node tests/comprehensive-api-tests.js security",
    "test:load": "node tests/comprehensive-api-tests.js load",
    "test:all": "node tests/comprehensive-api-tests.js all",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "postinstall": "pnpm dlx prisma generate",
    "prebuild": "pnpm dlx prisma generate",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "docker:build": "docker build -t hotel-management .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "backup:db": "pg_dump $DATABASE_URL > backup_$(date +%Y%m%d_%H%M%S).sql",
    "restore:db": "psql $DATABASE_URL < backup_file.sql",
    "analyze": "ANALYZE=true npm run build",
    "serve": "next start -p 3000",
    "security:scan": "npm audit --audit-level moderate",
    "security:fix:force": "npm audit fix --force",
    "security:headers": "node scripts/check-security-headers.js",
    "admin:setup": "tsx scripts/setup-admin-permissions.ts",
    "permissions:migrate": "tsx scripts/migrate-permissions.ts",
    "roles:create": "tsx scripts/create-roles.ts",
    "health:check": "node scripts/health-check.js",
    "monitor:start": "node scripts/monitoring-server.js",
    "backup:auto": "node scripts/auto-backup.js",
    "test:security:full": "node tests/security/comprehensive-security-test.js",
    "test:permissions": "node tests/comprehensive-api-tests.js permissions",
    "test:admin": "node tests/comprehensive-api-tests.js admin",
    "test:admin:full": "node tests/comprehensive-api-tests.js",
    "deploy:check": "node scripts/pre-deployment-check.js",
    "deploy:optimize": "npm run build && npm run analyze",
    "vercel:build": "npm run db:generate && npm run build",
    "vercel:deploy": "vercel --prod",
    "vercel:preview": "vercel",
    "vercel:dev": "vercel dev",
    "postbuild": "node scripts/post-build.js",
    "prestart": "npm run db:migrate:prod"
  },
